<div id="<%= dom_id mantainer %>" class="card mb-2">
  <div class="card-body">
    <p class="card-text">
      <strong>Type equipment:</strong>
      <%= mantainer.type_equipment %>
    </p>

    <p class="card-text">
      <strong>Type support:</strong>
      <%= mantainer.type_support %>
    </p>

    <p class="card-text">
      <strong>Name:</strong>
      <%= mantainer.name %>
    </p>

    <p class="card-text">
      <strong>City:</strong>
      <%= mantainer.city %>
    </p>

    <p class="card-text">
      <strong>Material:</strong>
      <%= mantainer.material %>
    </p>

 
    <% if mantainer.photo.present? %>
      <%= image_tag mantainer.photo.url, class: "card-img-top" %>
    <% else %>
      <p>No image available</p>
    <% end %>
  </div>
</div>
